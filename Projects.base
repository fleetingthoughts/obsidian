formulas:
  Progress: (file.backlinks.length/number(milestones)*100).round(2)+"%"
views:
  - type: table
    name: Table
    filters:
      and:
        - file.hasProperty("project")
    order:
      - file.name
      - deadline
      - milestones
      - formula.Progress
    sort:
      - property: formula.Progress
        direction: ASC
    columnSize:
      formula.Progress: 1784
