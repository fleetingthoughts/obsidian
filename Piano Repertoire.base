views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.contains("repertoire")
    order:
      - file.name
      - mastery
    sort:
      - property: mastery
        direction: DESC
    columnSize:
      file.name: 472
  - type: table
    name: intermediate practice
    filters:
      and:
        - file.tags.contains("#practice")
