views:
  - type: table
    name: Repertoire
    filters:
      and:
        - mastery.contains("repertoire")
    order:
      - file.name
      - RCM
      - ABRSM
    sort:
      - property: mastery
        direction: DESC
    columnSize:
      file.name: 472
  - type: table
    name: intermediate practice
    filters:
      and:
        - mastery.contains("practice")
    order:
      - file.name
      - RCM
      - ABRSM
    columnSize:
      file.name: 369
      note.RCM: 143
